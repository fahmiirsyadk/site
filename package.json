{
  "name": "site",
  "version": "1.0.2",
  "repository": "https://github.com/fahmiirsyadk/site",
  "author": "fahmiirsyadk <fahmiirsyad10@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@fahmiirsyadk/dust": "0.1.4",
    "rescript": "9.1.4"
  },
  "scripts": {
    "re:dev": "rescript build -w",
    "re:build": "rescript build",
    "re:clean": "rescript clean",
    "css:dev": "postcss src/public/assets/css/tailwind.css -o src/public/assets/css/styles.css",
    "dust:dev": "npx dust w",
    "dust:build": "npx dust",
    "dev": "yarn css:dev && yarn dust:dev",
    "export": "yarn re:build && yarn css:dev && yarn dust:build"
  },
  "dependencies": {
    "autoprefixer": "^10.3.7",
    "postcss": "^8.3.9",
    "postcss-cli": "^9.0.1",
    "tailwindcss": "^2.2.16"
  }
}
