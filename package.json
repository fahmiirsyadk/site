{
  "name": "site",
  "version": "1.0.2",
  "repository": "https://github.com/fahmiirsyadk/site",
  "author": "fahmiirsyadk <fahmiirsyad10@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@fahmiirsyadk/dust": "0.2.0",
    "rescript": "9.1.4"
  },
  "scripts": {
    "re:dev": "rescript build -w",
    "re:build": "rescript build",
    "re:clean": "rescript clean",
    "css:build": "tailwindcss --postcss -i src/public/assets/css/tailwind.css -o dist/assets/css/styles.css",
    "css:dev": "tailwindcss --postcss -i src/public/assets/css/tailwind.css -o src/public/assets/css/styles.css",
    "dust:dev": "dum dust w",
    "dust:build": "dum dust",
    "dev": "dum css:dev && dum dust:dev",
    "export": "export NODE_ENV=production && dum re:build && yarn dust:build && yarn css:build"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "4.1.3",
    "autoprefixer": "10.4.0",
    "cssnano": "5.0.12",
    "postcss": "8.4.4",
    "postcss-cli": "9.0.2",
    "postcss-import": "^14.0.2",
    "tailwindcss": "2.2.19"
  }
}
