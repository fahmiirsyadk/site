{
  "name": "site",
  "version": "1.0.2",
  "repository": "https://github.com/fahmiirsyadk/site",
  "author": "fahmiirsyadk <fahmiirsyad10@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@fahmiirsyadk/dust": "0.2.7",
    "rescript": "9.1.4"
  },
  "scripts": {
    "re:dev": "rescript build -w",
    "re:build": "rescript build",
    "re:clean": "rescript clean",
    "css:build": "tailwindcss -i tailwind/tailwind.css -o dist/assets/css/styles.css --minify",
    "css:dev": "tailwindcss -i tailwind/tailwind.css -o src/public/assets/css/styles.css -w",
    "dust:dev": "npx dust w",
    "dust:build": "npx dust",
    "export": "export NODE_ENV=production && yarn re:build && yarn dust:build && yarn css:build",
    "postexport": "yarn re:clean"
  },
  "dependencies": {
    "@tailwindcss/typography": "0.5.2",
    "tailwindcss": "3.0.24"
  }
}
